{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/old-path", "destination": "/new-path" }
  ],
  "redirects": [
    { "source": "/old-url", "destination": "/new-url", "permanent": true }
  ],
  "headers": [
    {
      "source": "/static/css/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    }
  ]
}